{
  "name": "pgrimaud/weibo-user-timeline",
  "type": "library",
  "description": "Easily fetch any user's weibo timeline without oAuth",
  "homepage": "https://github.com/pgrimaud/weibo-user-timeline",
  "license": "MIT",
  "authors": [
    {
      "name": "Pierre Grimaud",
      "email": "grimaud.pierre@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Weibo\\": "src/Weibo/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Weibo\\tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.6",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "friendsofphp/php-cs-fixer": "^2.4",
    "php-coveralls/php-coveralls": "^2.0"
  }
}
